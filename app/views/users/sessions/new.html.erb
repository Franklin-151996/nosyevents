  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="limiter">
    <div class="container-login100" style="background-image: url('images/bg-02.png');">
      <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
        <form class="login100-form validate-form">
          <span class="login100-form-title p-b-49">
            Se connecter
          </span>

          <div class="wrap-input100 validate-input m-b-23" data-validate = " Nom d'utilisateur ou email non valide">
            <label for="user_email" class="label-input100">Nom d'utilisateur ou Email</label>
            <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "input100", placeholder: "Entrer votre nom d'utilisateur ou email"%>
            <span class="focus-input100"></span>
          </div>

          <div class="wrap-input100 validate-input" data-validate="mots de passe non valide">
            <label for="user_password" class="label-input100">Mots de passe</label>
            <%= f.password_field :password, autocomplete: "current-password", class: "input100", placeholder: "Entrer votre mots de passe"%>
            <span class="focus-input100"></span>
          </div>

          <div class="text-right p-t-8 p-b-10">
            <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
              <%= link_to "Mots de passe oublier?", new_password_path(resource_name) %><br />
            <% end %>
          </div>
          
          <div class="text-right p-b-31">
            <% if devise_mapping.rememberable? %>
              <%= f.check_box :remember_me %>
              <label for="user_remember_me", style="color: #666666FF">Se souvenir de moi</label>
            <% end %>
          </div>

          
          <div class="container-login100-form-btn">
            <div class="wrap-login100-form-btn">
              <div class="login100-form-bgbtn"></div>
              <button class="login100-form-btn">
                Se connecter
              </button>
            </div>
          </div>

          <div class="txt1 text-center p-t-54 p-b-20">
            <span>
              Ou se connecter avec
            </span>
          </div>

          <div class="flex-c-m">
            <a href="#" class="login100-social-item bg1">

            </a>

            <a href="#" class="login100-social-item bg3">

            </a>
          </div>

          <div class="flex-col-c p-t-50">
            <span class="txt1 p-b-17">
              Pas inscrit?
            </span>

            <a href="<%= new_user_registration_path%>" class="txt2" style="color: #10F300FF">
              S'inscrire
            </a>
          </div>
        </form>
      </div>
    </div>
  </div>
<% end %>







